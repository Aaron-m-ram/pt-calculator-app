{
    "env": {
        "browser": true,
        "es2021": true
    },
    
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "import/no-extraneous-dependencies": ["warning"]
    }
}
